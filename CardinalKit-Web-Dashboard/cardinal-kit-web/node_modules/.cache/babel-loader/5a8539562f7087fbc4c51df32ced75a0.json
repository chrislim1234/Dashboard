{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Logo from \"@/components/auth/Logo\";\nimport Card from \"@/components/auth/Card\";\nimport store from \"@/store\";\nimport { mapActions, mapGetters } from \"vuex\";\nimport SocialButton from '../../components/auth/SocialButton.vue';\nexport default {\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      logo: require('@/assets/LogoCardinalKit@2x.png'),\n      showError: false\n    };\n  },\n\n  components: {\n    Logo,\n    Card,\n    SocialButton\n  },\n  computed: { ...mapGetters(\"auth\", [\"getError\"])\n  },\n  methods: { ...mapActions(\"auth\", [\"SignIn\", \"LogInWithGoogle\", \"LogInWithAppleId\"]),\n\n    handleSubmitLogin() {\n      this.SignIn({\n        email: this.email,\n        password: this.password\n      }).then(response => {\n        if (response.isLogged) {\n          this.$router.push({\n            name: \"Home\"\n          });\n        }\n      });\n    },\n\n    handleGoogleLogin() {\n      this.showError = false;\n      this.LogInWithGoogle().then(response => {\n        if (response.isLogged) {\n          this.$router.push({\n            name: \"Home\"\n          });\n        }\n      });\n    },\n\n    handleAppleLogin() {\n      this.LogInWithAppleId().then(response => {\n        if (response.isLogged) {\n          this.$router.push({\n            name: \"Home\"\n          });\n        }\n      }).catch(error => console.log('Error from Server', error));\n    }\n\n  }\n};","map":{"version":3,"sources":["/Users/chrislim/Desktop/USC/Senior Fall/CSCI401/CardinalKit-Web-Dashboard/cardinal-kit-web/src/views/auth/login.vue"],"names":[],"mappings":";AAqEA,OAAO,IAAP,MAAiB,wBAAjB;AACA,OAAO,IAAP,MAAiB,wBAAjB;AACA,OAAO,KAAP,MAAkB,SAAlB;AACA,SAAS,UAAT,EAAqB,UAArB,QAAuC,MAAvC;AACA,OAAO,YAAP,MAAyB,wCAAzB;AAEA,eAAe;EACb,IAAI,GAAG;IACL,OAAO;MACL,KAAK,EAAE,EADF;MAEL,QAAQ,EAAE,EAFL;MAGL,IAAI,EAAE,OAAO,CAAC,iCAAD,CAHR;MAIL,SAAS,EAAE;IAJN,CAAP;EAMD,CARY;;EASb,UAAU,EAAE;IACV,IADU;IAEV,IAFU;IAGV;EAHU,CATC;EAcb,QAAQ,EAAC,EACP,GAAG,UAAU,CAAC,MAAD,EAAQ,CAAC,UAAD,CAAR;EADN,CAdI;EAiBb,OAAO,EAAE,EACP,GAAG,UAAU,CAAC,MAAD,EAAS,CAAC,QAAD,EAAU,iBAAV,EAA6B,kBAA7B,CAAT,CADN;;IAEP,iBAAiB,GAAG;MAClB,KAAK,MAAL,CAAY;QAAE,KAAK,EAAE,KAAK,KAAd;QAAqB,QAAQ,EAAE,KAAK;MAApC,CAAZ,EACC,IADD,CACO,QAAD,IAAY;QAChB,IAAG,QAAQ,CAAC,QAAZ,EAAqB;UACnB,KAAK,OAAL,CAAa,IAAb,CAAkB;YAAC,IAAI,EAAC;UAAN,CAAlB;QACF;MACD,CALD;IAMD,CATM;;IAUP,iBAAiB,GAAE;MACjB,KAAK,SAAL,GAAiB,KAAjB;MACA,KAAK,eAAL,GACC,IADD,CACO,QAAD,IAAY;QAChB,IAAG,QAAQ,CAAC,QAAZ,EAAqB;UACnB,KAAK,OAAL,CAAa,IAAb,CAAkB;YAAC,IAAI,EAAC;UAAN,CAAlB;QACF;MACD,CALD;IAMD,CAlBM;;IAmBP,gBAAgB,GAAG;MACjB,KAAK,gBAAL,GACC,IADD,CACO,QAAD,IAAY;QAChB,IAAG,QAAQ,CAAC,QAAZ,EAAqB;UACnB,KAAK,OAAL,CAAa,IAAb,CAAkB;YAAE,IAAI,EAAE;UAAR,CAAlB;QACF;MACD,CALD,EAMC,KAND,CAMQ,KAAD,IAAW,OAAO,CAAC,GAAR,CAAY,mBAAZ,EAAiC,KAAjC,CANlB;IAOF;;EA3BO;AAjBI,CAAf","sourcesContent":["<template>\n  <section class=\"wrapper-login\">\n    <Card>\n      <template v-slot:card-header>\n        <Logo :path=\"logo\" :className=\"'text-center'\" :width=\"'180'\"></Logo>\n      </template>\n      <template v-slot:card-body>\n        <br>\n        <h4 class=\"my-2 text-muted\">Sign In</h4>\n        <form class=\"login-form\" @submit.prevent=\"handleSubmitLogin\">\n          <div v-if=\"getError.error\" className=\"auth__error\">\n            <p class=\"mb-1 subtitle\">{{getError.errorMessage}}</p>\n          </div>\n          <div class=\"form-group\">\n            <label for=\"email\">Email Address</label>\n            <input\n              type=\"email\"\n              placeholder=\"\"\n              id=\"email\"\n              v-model=\"email\"\n            />\n          </div>\n          <div class=\"form-group\">\n            <label for=\"password\">Password</label>\n            <input\n              type=\"password\"\n              placeholder=\"\"\n              id=\"password\"\n              v-model=\"password\"\n              autocomplete=\"on\"\n            />\n            <router-link to=\"#\" class=\"ev-link forgot \"\n              >Forgot Password?</router-link\n            >\n          </div>\n          <button \n            class=\"btn btn-ck fill-danger w-50 m-auto\"\n            type=\"submit\"\n          >\n            Sign in\n          </button>\n        </form>\n      </template>\n      <hr />\n      <template v-slot:card-footer>\n        <div className=\"auth__social-networks\">\n          <p class=\"mb-1 subtitle\">Login with social networks</p>\n          <social-button \n            @click=\"handleGoogleLogin\"\n            icon=\"https://upload.wikimedia.org/wikipedia/commons/5/53/Google_%22G%22_Logo.svg\" \n            text=\"Sign in with google\"\n            className=\"btn btn-ck otl-danger\"\n          />\n          <social-button\n            @click=\"handleAppleLogin\" \n            icon=\"https://upload.wikimedia.org/wikipedia/commons/thumb/f/fa/Apple_logo_black.svg/240px-Apple_logo_black.svg.png\" \n            text=\"Sign in with apple\"\n            className=\"btn btn-ck otl-danger\"\n          />\n        </div>\n        <!-- <div class=\"text-center\">\n          <span class=\"ft-10\">Not a member? </span>\n          <router-link class=\"ev-link\" to=\"/signup\">Sign up</router-link>\n        </div> -->\n      </template>\n    </Card>\n  </section>\n</template>\n<script>\nimport Logo from \"@/components/auth/Logo\";\nimport Card from \"@/components/auth/Card\";\nimport store from \"@/store\"\nimport { mapActions, mapGetters } from \"vuex\";\nimport SocialButton from '../../components/auth/SocialButton.vue';\n\nexport default {\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      logo: require('@/assets/LogoCardinalKit@2x.png'),\n      showError: false\n    };\n  },\n  components: {\n    Logo,\n    Card,\n    SocialButton,\n  },\n  computed:{\n    ...mapGetters(\"auth\",[\"getError\"])\n  },\n  methods: {\n    ...mapActions(\"auth\", [\"SignIn\",\"LogInWithGoogle\", \"LogInWithAppleId\"]),\n    handleSubmitLogin() {\n      this.SignIn({ email: this.email, password: this.password })\n      .then((response)=>{\n        if(response.isLogged){\n          this.$router.push({name:\"Home\"});\n        }\n      })\n    },\n    handleGoogleLogin(){\n      this.showError = false\n      this.LogInWithGoogle()\n      .then((response)=>{\n        if(response.isLogged){\n          this.$router.push({name:\"Home\"});\n        }\n      })\n    },\n    handleAppleLogin() {\n      this.LogInWithAppleId()\n      .then((response)=>{\n        if(response.isLogged){\n          this.$router.push({ name: \"Home\" });\n        }\n      })\n      .catch((error) => console.log('Error from Server', error))\n    }\n  },\n};\n</script>\n<style lang=\"scss\" scoped>\n.wrapper-login {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  border-radius: 2px;\n  height: 100vh;\n  background: $background-login;\n\n  .login-form {\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    margin-bottom: 2rem;\n\n    .form-group {\n      margin-bottom: 1rem;\n      display: flex;\n      flex-direction: column;\n\n      label {\n        text-transform: uppercase;\n        text-align: start;\n        margin-bottom: 3px;\n        font-size: 10pt;\n        color: #6c757d\n      }\n      input {\n        margin-bottom: 0.5rem;\n        font-size: 1em;\n        border: 0;\n        border-bottom: 1px solid #6c757d;\n        -webkit-appearance: none;\n        border-radius: 0;\n        padding: 0;\n        cursor: text;\n      }:focus {\n        outline: 0;\n        border-bottom: 1px solid #B61440;\n      }\n\n      a {\n        width: 100%;\n        display: flex;\n        justify-content: flex-end;\n\n        &.forgot {\n          font-size: 10pt;\n          color: #6c757d\n        }\n      }\n    }\n  }\n  \n  .auth__social-networks {\n    margin-bottom: .5rem;\n    display: grid;\n    gap: 10px;\n\n    .subtitle {\n      margin-top: 0;\n      text-align: center;\n    }\n  }\n  .auth__error {\n    margin-bottom: .5rem;\n    display: grid;\n    gap: 10px;\n    margin-top: 0;\n    text-align: center;\n    color: red;\n  }\n}\n\n</style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}