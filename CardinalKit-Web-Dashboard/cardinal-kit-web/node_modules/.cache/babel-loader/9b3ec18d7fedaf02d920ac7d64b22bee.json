{"ast":null,"code":"import Card from \"@/components/auth/Card\";\nimport Multiselect from \"@/components/multiSelect/Multiselect\";\nimport { mapActions, mapGetters } from \"vuex\";\nimport store from \"@/store\";\nexport default {\n  data() {\n    return {\n      email: \"\",\n      studies: []\n    };\n  },\n\n  components: {\n    Card,\n    Multiselect\n  },\n  methods: { ...mapActions(\"auth\", [\"SingUpNoPassword\"]),\n\n    handleSubmitRegister() {\n      this.SingUpNoPassword({\n        email: email.value,\n        studies: this.studies\n      }).then(() => {\n        this.studies = [];\n        this.email = \"\";\n        this.$notify({\n          title: \"Success\",\n          text: \"The user was successfully registered\"\n        });\n      }).catch(error => {\n        this.$notify({\n          title: \"Error\",\n          text: error\n        });\n      });\n    }\n\n  },\n  computed: { ...mapGetters(\"user\", [\"getUserStudies\"])\n  },\n\n  beforeRouteEnter(to, from, next) {\n    Promise.all([store.dispatch(\"user/FetchUserRolesAndStudies\")]).then(() => {\n      next();\n    });\n  }\n\n};","map":{"version":3,"sources":["/Users/chrislim/Desktop/USC/Senior Fall/CSCI401/CardinalKit-Web-Dashboard/cardinal-kit-web/src/views/auth/registerDoctor.vue"],"names":[],"mappings":"AAsCA,OAAO,IAAP,MAAiB,wBAAjB;AACA,OAAO,WAAP,MAAwB,sCAAxB;AACA,SAAS,UAAT,EAAqB,UAArB,QAAuC,MAAvC;AACA,OAAO,KAAP,MAAkB,SAAlB;AAEA,eAAe;EACb,IAAI,GAAG;IACL,OAAO;MACL,KAAK,EAAE,EADF;MAEL,OAAO,EAAE;IAFJ,CAAP;EAID,CANY;;EAOb,UAAU,EAAE;IACV,IADU;IAEV;EAFU,CAPC;EAWb,OAAO,EAAE,EACP,GAAG,UAAU,CAAC,MAAD,EAAS,CAAC,kBAAD,CAAT,CADN;;IAEP,oBAAoB,GAAG;MACrB,KAAK,gBAAL,CAAsB;QACpB,KAAK,EAAE,KAAK,CAAC,KADO;QAEpB,OAAO,EAAE,KAAK;MAFM,CAAtB,EAGG,IAHH,CAGQ,MAAM;QACZ,KAAK,OAAL,GAAa,EAAb;QACA,KAAK,KAAL,GAAW,EAAX;QACA,KAAK,OAAL,CAAa;UACX,KAAK,EAAE,SADI;UAEX,IAAI,EAAE;QAFK,CAAb;MAID,CAVD,EAUG,KAVH,CAUU,KAAD,IAAS;QAChB,KAAK,OAAL,CAAa;UACX,KAAK,EAAE,OADI;UAEX,IAAI,EAAE;QAFK,CAAb;MAID,CAfD;IAgBD;;EAnBM,CAXI;EAgCb,QAAQ,EAAE,EACR,GAAG,UAAU,CAAC,MAAD,EAAS,CAAC,gBAAD,CAAT;EADL,CAhCG;;EAmCb,gBAAgB,CAAC,EAAD,EAAK,IAAL,EAAW,IAAX,EAAiB;IAC/B,OAAO,CAAC,GAAR,CAAY,CAAC,KAAK,CAAC,QAAN,CAAe,+BAAf,CAAD,CAAZ,EAA+D,IAA/D,CAAoE,MAAM;MACxE,IAAI;IACL,CAFD;EAGD;;AAvCY,CAAf","sourcesContent":["<template>\n  <section class=\"wrapper-login\">\n    <Card>\n      <template v-slot:card-header>\n        <div class=\"card-header\">\n          <h1 id=\"signup\" class=\"mb-1 text-center text-muted\">Register Doctors</h1>\n        </div>\n      </template>\n      <template v-slot:card-body>\n        <form class=\"login-form\" @submit.prevent=\"handleSubmitRegister\">\n          <div class=\"form-group__horizontal\"></div>\n          <div class=\"w-100 form-group\">\n            <label for=\"email\" class=\"text-muted\">EMAIL</label>\n            <input\n              class=\"form-input\"\n              type=\"email\"\n              placeholder=\"\"\n              id=\"email\"\n              v-model=\"email\"\n              required\n            />\n          </div>\n          <div class=\"w-100 form-group\">\n            <label for=\"studies\" class=\"text-muted\">STUDY</label>\n            <Multiselect :options=\"getUserStudies\" v-model=\"studies\" />\n          </div>\n          <button id=\"b-signup\"\n            class=\"btn btn-ck fill-danger w-50 m-auto\"\n            type=\"submit\">\n            Register\n          </button>\n        </form>\n      </template>\n    </Card>\n  </section>\n</template>\n<script>\n\nimport Card from \"@/components/auth/Card\";\nimport Multiselect from \"@/components/multiSelect/Multiselect\";\nimport { mapActions, mapGetters } from \"vuex\";\nimport store from \"@/store\";\n\nexport default {\n  data() {\n    return {\n      email: \"\",\n      studies: []\n    };\n  },\n  components: {\n    Card,\n    Multiselect,\n  },\n  methods: {\n    ...mapActions(\"auth\", [\"SingUpNoPassword\"]),\n    handleSubmitRegister() {\n      this.SingUpNoPassword({\n        email: email.value,\n        studies: this.studies,\n      }).then(() => {\n        this.studies=[]\n        this.email=\"\"\n        this.$notify({\n          title: \"Success\",\n          text: \"The user was successfully registered\",\n        });\n      }).catch((error)=>{\n        this.$notify({\n          title: \"Error\",\n          text: error,\n        });\n      });\n    },\n  },\n  computed: {\n    ...mapGetters(\"user\", [\"getUserStudies\"]),\n  },\n  beforeRouteEnter(to, from, next) {\n    Promise.all([store.dispatch(\"user/FetchUserRolesAndStudies\")]).then(() => {\n      next();\n    });\n  },\n};\n</script>\n<style lang=\"scss\" scoped>\n.wrapper-login {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  border-radius: 2px;\n  height: 100vh;\n  background: $background-signup;\n\n  .card-header {\n    margin-bottom: 2rem;\n\n    h1#signup {\n      font-size: 2rem;\n    }\n\n    span {\n      font-size: 10pt;\n    }\n    @media screen and (max-width: 510px) {\n      margin-bottom: 1rem;\n    }\n  }\n\n  .login-form {\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    margin-bottom: 1rem;\n\n    .form-group {\n      margin-bottom: 1rem;\n      display: flex;\n      flex-direction: column;\n\n      &__horizontal {\n        display: flex;\n        gap: 15px;\n\n        @media screen and (max-width: 510px) {\n          flex-direction: column;\n        }\n      }\n\n      label {\n        text-align: start;\n        margin-bottom: 3px;\n        font-size: 10pt;\n      }\n\n      a {\n        width: 100%;\n        display: flex;\n        justify-content: flex-end;\n      }\n    }\n    button#b-signup {\n      margin-top: 1rem;\n    }\n  }\n}\n</style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}