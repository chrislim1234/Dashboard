{\n    showToGraph(id) {\n      this.loading=true\n      this.$router.push({\n        name: \"statistic\",\n        params: {hkCode: id},\n        query: {studyId: this.studyId, userId: this.userId}\n      });\n    }\n  }\n};\n</script>\n<style lang=\"scss\" scoped>\n.width {\n  width: 13rem;\n}\n.card {\n  box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2);\n  transition: 0.3s;\n  min-width: 40%;\n  padding-bottom: 5px;\n  border-radius: 5px;\n  height: auto;\n  background-color: $bg-card;\n  justify-content: space-between;\n\n  .card-header {\n    //display: flex;\n    justify-content: space-between;\n    padding: .7rem;\n\n    &__title {\n      display: flex;\n      align-items: center;\n\n      img {\n        margin-right: 5px;\n        object-fit: scale-down;\n        height: 17px;\n        width: 12px;\n      }\n    }\n\n    &__link {\n      .arrow-link {\n        text-decoration: none;\n        font-weight: bold;\n        color: lightgrey;\n        margin-left: 5px;\n        font-size: 15pt;\n      }\n    }\n  }\n\n  .card-body {\n    &__info {\n      display: flex;\n      gap: 1px;\n      padding: .7rem;\n      align-items: baseline;\n\n      .info-value {\n        font-size: 17pt;\n        font-weight: 700;\n        text-transform: uppercase;\n      }\n      .info-measure {\n        color: lightslategray;\n        font-weight: bold;\n        font-size: 10pt;\n      }\n    }\n  }\n}\n\n.card:hover {\n  box-shadow: 0 8px 16px 0 rgba(0,0,0,0.2);\n}\n\nimg {\n  border-radius: 5px 5px 0 0;\n}\n\n.container {\n  padding: 2px 16px;\n}\n</style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}