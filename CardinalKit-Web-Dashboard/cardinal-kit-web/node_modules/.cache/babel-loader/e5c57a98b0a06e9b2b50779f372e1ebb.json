{"ast":null,"code":"import Logo from '@/components/auth/Logo';\nimport Card from '@/components/auth/Card';\nimport { mapActions } from 'vuex';\nexport default {\n  data() {\n    return {\n      email: '',\n      password: '',\n      firstName: '',\n      lastName: '',\n      logo: require('@/assets/logo.png')\n    };\n  },\n\n  components: {\n    Logo,\n    Card\n  },\n  methods: { ...mapActions('auth', ['SignUp']),\n\n    handleSubmitRegister() {\n      this.SignUp({\n        email: email.value,\n        password: password.value,\n        firstName: firstName.value,\n        lastName: lastName.value\n      });\n    }\n\n  }\n};","map":{"version":3,"sources":["/Users/chrislim/Desktop/USC/Senior Fall/CSCI401/CardinalKit-Web-Dashboard/cardinal-kit-web/src/views/auth/signUp.vue"],"names":[],"mappings":"AAqEA,OAAO,IAAP,MAAiB,wBAAjB;AACA,OAAO,IAAP,MAAiB,wBAAjB;AACA,SAAS,UAAT,QAA2B,MAA3B;AAEA,eAAe;EACb,IAAI,GAAE;IACJ,OAAM;MACJ,KAAK,EAAC,EADF;MAEJ,QAAQ,EAAE,EAFN;MAGJ,SAAS,EAAE,EAHP;MAIJ,QAAQ,EAAE,EAJN;MAKJ,IAAI,EAAE,OAAO,CAAC,mBAAD;IALT,CAAN;EAOD,CATY;;EAUb,UAAU,EAAC;IACT,IADS;IAET;EAFS,CAVE;EAcb,OAAO,EAAC,EACN,GAAG,UAAU,CAAC,MAAD,EAAQ,CAAC,QAAD,CAAR,CADP;;IAEN,oBAAoB,GAAE;MACpB,KAAK,MAAL,CAAY;QACV,KAAK,EAAE,KAAK,CAAC,KADH;QAEV,QAAQ,EAAE,QAAQ,CAAC,KAFT;QAGV,SAAS,EAAE,SAAS,CAAC,KAHX;QAIV,QAAQ,EAAE,QAAQ,CAAC;MAJT,CAAZ;IAMF;;EATM;AAdK,CAAf","sourcesContent":["<template>\n  <section class=\"wrapper-login\">\n    <Card>\n      <template v-slot:card-header>\n        <div class=\"card-header\">\n          <Logo :path=\"logo\" :width=\"'100'\" :className=\"'flex justify-center'\"></Logo>\n          <h1 id=\"signup\" class=\"mb-1 text-center\">Register doctor</h1>\n          <!-- <span class=\"block text-center\">\n            Already have an account?\n            <router-link class=\"ev-link\" to=\"/login\">Sign in</router-link>\n          </span> -->\n        </div>\n      </template>\n      <template v-slot:card-body>\n        <form class=\"login-form\" @submit.prevent=\"handleSubmitRegister\">\n          <div class=\"form-group__horizontal\">\n            <div class=\"w-100 form-group\">\n              <label for=\"firstName\">First Name</label>\n              <input\n                class=\"form-input\"\n                type=\"text\"\n                placeholder=\"\"\n                id=\"firstName\"\n                v-model=\"firstName\"\n              />\n            </div>\n            <div class=\"w-100 form-group\">\n              <label for=\"lastName\">Last Name</label>\n              <input\n                class=\"form-input\"\n                type=\"text\"\n                placeholder=\"\"\n                id=\"lastName\"\n                v-model=\"lastName\"\n              />\n            </div>\n          </div>\n          <div class=\"w-100 form-group\">\n            <label for=\"email\">Email Address</label>\n            <input\n              class=\"form-input\"\n              type=\"email\"\n              placeholder=\"\"\n              id=\"email\"\n              v-model=\"email\"\n              required\n            />\n          </div>\n          <!-- <div class=\"form-group\">\n            <label for=\"password\">Password</label>\n            <input\n              class=\"form-input\"\n              type=\"password\"\n              placeholder=\"\"\n              id=\"password\"\n              v-model=\"password\"\n              required\n              autocomplete=\"on\"\n            />\n          </div> -->\n          <button id=\"b-signup\" class=\"btn btn-primary\" type=\"submit\">\n            Register\n          </button>\n        </form>\n      </template>\n    </Card>\n  </section>\n</template>\n<script>\nimport Logo from '@/components/auth/Logo';\nimport Card from '@/components/auth/Card';\nimport { mapActions } from 'vuex'\n\nexport default({\n  data(){\n    return{\n      email:'',\n      password: '',\n      firstName: '',\n      lastName: '',\n      logo: require('@/assets/logo.png')\n    }\n  },\n  components:{\n    Logo,\n    Card\n  },\n  methods:{\n    ...mapActions('auth',['SignUp']),\n    handleSubmitRegister(){\n      this.SignUp({\n        email: email.value,\n        password: password.value,\n        firstName: firstName.value,\n        lastName: lastName.value,\n      })\n    }\n  }\n})\n\n</script>\n<style lang=\"scss\" scoped>\n.wrapper-login {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  border-radius: 2px;\n  height: 100vh;\n  background: $background-signup;\n\n  .card-header {\n    margin-bottom: 2rem;\n\n    h1#signup {\n      font-size: 2rem;\n    }\n\n    span {\n      font-size: 10pt;\n    }\n     @media screen and (max-width: 510px) {\n       margin-bottom: 1rem;\n    }\n  }\n\n  .login-form {\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    margin-bottom: 1rem;\n\n    .form-group {\n      margin-bottom: 1rem;\n      display: flex;\n      flex-direction: column;\n\n      &__horizontal {\n        display: flex;\n        gap: 15px;\n\n        @media screen and (max-width: 510px) {\n          flex-direction: column;\n        }\n      }\n\n      label {\n        text-align: start;\n        margin-bottom: 3px;\n        font-size: 10pt;\n      }\n\n      a {\n        width: 100%;\n        display: flex;\n        justify-content: flex-end;\n      }\n    }\n    button#b-signup {\n      margin-top: 1rem;\n    }\n  }\n}\n</style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}