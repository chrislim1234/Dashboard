{"ast":null,"code":"import store from \"@/store\";\nimport activityCard from \"@/components/patients/healthKit/activity/activityCard\";\nimport multipleRadialBars from \"@/components/apexCharts/multipleRadialBars\";\nimport { ACTIVITIES_PER_USER } from \"@/plugins/mock/activities\";\nimport { mapGetters } from 'vuex';\nexport default {\n  name: \"activity\",\n  components: {\n    activityCard,\n    multipleRadialBars\n  },\n\n  data() {\n    return {\n      calories: 39,\n      exerciseTime: 10,\n      standUpTime: 2,\n      activities: [],\n      categoryId: this.$route.params.categoryId\n    };\n  },\n\n  methods: {},\n  computed: { ...mapGetters('patient', ['getCategoryDataWebFormat'])\n  },\n\n  mounted() {\n    this.activities = this.getCategoryDataWebFormat(this.categoryId);\n  },\n\n  beforeRouteEnter(to, from, next) {\n    if (to.params.categoryId == \"survey\") {} else {\n      Promise.all([store.dispatch(\"patient/FetchLastCategoryData\", {\n        studyId: `${to.query.studyId}`,\n        userId: `${to.query.userId}`,\n        category: `${to.params.categoryId}`\n      })]).then(() => {\n        next();\n      });\n    }\n  }\n\n};","map":{"version":3,"sources":["/Users/chrislim/Desktop/USC/Senior Fall/CSCI401/CardinalKit-Web-Dashboard/cardinal-kit-web/src/views/patients/healthKit/categoryDetail.vue"],"names":[],"mappings":"AAuCA,OAAO,KAAP,MAAkB,SAAlB;AACA,OAAO,YAAP,MAAyB,uDAAzB;AACA,OAAO,kBAAP,MAA+B,4CAA/B;AACA,SAAS,mBAAT,QAAoC,2BAApC;AAEA,SAAS,UAAT,QAA2B,MAA3B;AACA,eAAe;EACb,IAAI,EAAE,UADO;EAEb,UAAU,EAAE;IACV,YADU;IAEV;EAFU,CAFC;;EAMb,IAAI,GAAG;IACL,OAAO;MACL,QAAQ,EAAC,EADJ;MAEL,YAAY,EAAC,EAFR;MAGL,WAAW,EAAC,CAHP;MAIL,UAAU,EAAE,EAJP;MAKL,UAAU,EAAE,KAAK,MAAL,CAAY,MAAZ,CAAmB;IAL1B,CAAP;EAOD,CAdY;;EAeb,OAAO,EAAE,EAfI;EAgBb,QAAQ,EAAE,EACR,GAAG,UAAU,CAAC,SAAD,EAAW,CAAC,0BAAD,CAAX;EADL,CAhBG;;EAmBb,OAAO,GAAE;IACP,KAAK,UAAL,GAAkB,KAAK,wBAAL,CAA8B,KAAK,UAAnC,CAAlB;EACD,CArBY;;EAsBb,gBAAgB,CAAC,EAAD,EAAK,IAAL,EAAW,IAAX,EAAiB;IAC/B,IAAG,EAAE,CAAC,MAAH,CAAU,UAAV,IAAsB,QAAzB,EAAkC,CAClC,CADA,MAGA;MACE,OAAO,CAAC,GAAR,CAAY,CACV,KAAK,CAAC,QAAN,CAAe,+BAAf,EACA;QAAE,OAAO,EAAE,GAAE,EAAE,CAAC,KAAH,CAAS,OAAQ,EAA9B;QAAiC,MAAM,EAAE,GAAE,EAAE,CAAC,KAAH,CAAS,MAAO,EAA3D;QAA6D,QAAQ,EAAE,GAAE,EAAE,CAAC,MAAH,CAAU,UAAW;MAA9F,CADA,CADU,CAAZ,EAGG,IAHH,CAGQ,MAAI;QACV,IAAI;MACL,CALD;IAMF;EACD;;AAlCY,CAAf","sourcesContent":["<template>\n  <section class=\"page\">\n    <!-- <h1 class=\"mb-5 text-capitalize\"> {{categoryId}}</h1> -->\n    <div class=\"row\">\n       <!-- <activity-card name=\"Actividad\" date=\"7 may\">\n        <template v-slot:card-body>\n          <div class=\"card-info\">\n            <div class=\"card-info__group\">\n              <p class=\"subtitle\">move</p>\n              <p>{{calories}} kcal</p>\n            </div>\n            <div class=\"card-info__group\">\n              <p class=\"subtitle\">exercise</p>\n              <p>{{exerciseTime}} min</p>\n            </div>\n            <div class=\"card-info__group\">\n              <p class=\"subtitle\">Stand Up</p>\n              <p>{{standUpTime}} h</p>\n            </div>\n          </div>\n           <multiple-radial-bars :series=\"[calories,exerciseTime,standUpTime]\" :height=\"'250'\" :labels=\"['Move','exercise','Stand Up']\"/> \n        </template>\n      </activity-card> -->\n        <div class=\"col flex wrap\" v-if=\"activities && activities.length\" >\n          <activity-card \n            v-for=\"activity in activities\" :key=\"activity.HkCode\"\n            :name=\"activity.HkCodeName\" \n            :date=\"activity.Date.formatted\"\n            :color-title=\"activity.Color\" \n            :value=\"''+activity.Value\" \n            :measure=\"activity.Unit\"\n            :logo=\"activity.Logo\"\n            :id=\"activity.HkCode\">\n          </activity-card>\n        </div>\n    </div>\n  </section>\n</template>\n<script>\nimport store from \"@/store\";\nimport activityCard from \"@/components/patients/healthKit/activity/activityCard\";\nimport multipleRadialBars from \"@/components/apexCharts/multipleRadialBars\";\nimport { ACTIVITIES_PER_USER } from \"@/plugins/mock/activities\";\n\nimport { mapGetters } from 'vuex';\nexport default {\n  name: \"activity\",\n  components: {\n    activityCard,\n    multipleRadialBars\n  },\n  data() {\n    return {\n      calories:39,\n      exerciseTime:10,\n      standUpTime:2,\n      activities: [],\n      categoryId: this.$route.params.categoryId\n    };\n  },\n  methods: {},\n  computed: {\n    ...mapGetters('patient',['getCategoryDataWebFormat'])\n  },\n  mounted(){\n    this.activities = this.getCategoryDataWebFormat(this.categoryId)\n  },\n  beforeRouteEnter(to, from, next) {\n    if(to.params.categoryId==\"survey\"){\n    }\n    else\n    {\n      Promise.all([\n        store.dispatch(\"patient/FetchLastCategoryData\",\n        { studyId:`${to.query.studyId}`, userId:`${to.query.userId}`,category:`${to.params.categoryId}`})\n      ]).then(()=>{\n        next()\n      })\n    }\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.wrap {\n  flex-flow: wrap;\n}\n\n.card-info {\n  padding: 1rem;\n  display: flex;\n  justify-content: space-between;\n\n  &__group {\n    text-align: center;\n    padding: 1rem;\n    width: 100%;\n    \n    .subtitle {\n      font-weight: bold;\n      margin-bottom: .8rem;\n      text-transform: capitalize;\n    }\n\n    &:nth-child(n) {\n      border-right: solid lightgray 1px;\n    }\n    &:last-child {\n      border: none;\n    }\n  }\n}\n</style>"],"sourceRoot":""},"metadata":{},"sourceType":"module"}